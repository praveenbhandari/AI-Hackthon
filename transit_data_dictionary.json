{
  "transit_data_dictionary": {
    "description": "Comprehensive dictionary of transit data structures and examples for BART system",
    "version": "1.0.0",
    "last_updated": "2024-12-19",
    
    "data_structures": {
      "transit_route": {
        "description": "Complete structure for a single transit route",
        "fields": {
          "route_id": {
            "type": "string",
            "description": "Unique identifier for the route",
            "examples": ["BART_001", "BART_FAST_001", "BART_CHEAP_001"],
            "required": true
          },
          "departure_time": {
            "type": "string",
            "description": "Route departure time in HH:MM format",
            "examples": ["09:00", "14:30", "23:45"],
            "required": true
          },
          "arrival_time": {
            "type": "string", 
            "description": "Route arrival time in HH:MM format",
            "examples": ["09:45", "15:15", "00:30"],
            "required": true
          },
          "time_taken": {
            "type": "string",
            "description": "Total journey time",
            "examples": ["45 minutes", "1 hour 15 minutes", "2 hours 30 minutes"],
            "required": true
          },
          "cost": {
            "type": "string",
            "description": "Total fare cost",
            "examples": ["5.00", "12.50", "3.75"],
            "required": true
          },
          "stops": {
            "type": "array",
            "description": "Array of stops along the route",
            "required": true,
            "items": {
              "type": "object",
              "properties": {
                "stop_number": {
                  "type": "string",
                  "description": "Stop identifier",
                  "examples": ["SFO", "DC", "EMB", "CIV"]
                },
                "stop_name": {
                  "type": "string", 
                  "description": "Full stop name",
                  "examples": ["San Francisco Airport", "Daly City", "Embarcadero"]
                },
                "arrival_time": {
                  "type": "string",
                  "description": "Arrival time at this stop",
                  "examples": ["09:00", "09:15", "09:45"]
                },
                "departure_time": {
                  "type": "string",
                  "description": "Departure time from this stop", 
                  "examples": ["09:05", "09:18", "09:45"]
                }
              }
            }
          },
          "total_stops": {
            "type": "integer",
            "description": "Number of stops on the route",
            "examples": [2, 5, 8],
            "required": true
          },
          "route_type": {
            "type": "string",
            "description": "Type of transit system",
            "examples": ["BART", "Bus", "Train"],
            "required": true
          },
          "optimization_type": {
            "type": "string",
            "description": "Type of optimization applied (for route optimizer)",
            "examples": ["fastest", "cheapest", "balanced"],
            "required": false
          },
          "efficiency_score": {
            "type": "integer",
            "description": "Efficiency score from 0-100 (for route optimizer)",
            "examples": [85, 92, 78],
            "required": false
          }
        }
      },
      
      "transit_routes_response": {
        "description": "Complete response structure for transit planning",
        "fields": {
          "transit_routes": {
            "type": "array",
            "description": "Array of transit route objects",
            "required": true
          },
          "summary": {
            "type": "object",
            "description": "Summary of all routes",
            "required": true,
            "properties": {
              "total_routes": {
                "type": "integer",
                "description": "Total number of routes found"
              },
              "fastest_route": {
                "type": "string",
                "description": "Route ID of fastest route"
              },
              "cheapest_route": {
                "type": "string", 
                "description": "Route ID of cheapest route"
              },
              "recommendation": {
                "type": "string",
                "description": "Human-readable recommendation"
              }
            }
          }
        }
      },
      
      "optimization_response": {
        "description": "Complete response structure for route optimization",
        "fields": {
          "optimized_routes": {
            "type": "array",
            "description": "Array of optimized route objects",
            "required": true
          },
          "optimization_summary": {
            "type": "object",
            "description": "Summary of optimization results",
            "required": true,
            "properties": {
              "fastest_route": {
                "type": "object",
                "properties": {
                  "route_id": {"type": "string"},
                  "time_taken": {"type": "string"},
                  "cost": {"type": "string"}
                }
              },
              "cheapest_route": {
                "type": "object", 
                "properties": {
                  "route_id": {"type": "string"},
                  "time_taken": {"type": "string"},
                  "cost": {"type": "string"}
                }
              },
              "recommended_route": {
                "type": "object",
                "properties": {
                  "route_id": {"type": "string"},
                  "reason": {"type": "string"}
                }
              }
            }
          }
        }
      }
    },
    
